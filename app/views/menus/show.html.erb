<div>
<h1> Menu Listings</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Cuisine</th>
    <th>Price</th>
    <th>Peanuts</th>
    <th>Gluent</th>
    <th>Diary</th>
    <th>Delete</th>
  </tr>
  <tr>
    <td><%= @menu.name %></td>
    <td><%= @menu.cuisine %></td>
    <td><%= @menu.price %></td>
    <td><%= @menu.peanuts %></td>
    <td><%= @menu.gluent %></td>
    <td><%= @menu.dairy %></td>
    <td><button><%= link_to "Delete", menu_path(@menu), method: :delete %></button></td>

  </tr>
</table>
</div>
