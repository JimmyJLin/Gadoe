
<div>
<h2>Current Parties</h2>
<table>
  <tr>
    <th>Table Number</th>
    <th># of Guests</th>
  </tr>
  <tr>
    <!-- <%= Table.all.each do |table| %> -->

    <td><%= table.table_num %></td>
    <td><%= table.guests %></td>
    <td><button><%= link_to "Delete", table_path(table) %></button></td>
    <td><button><%= link_to "Edit", edit_table_path(table) %></button></td>

  </tr>
  <!-- <% end %> -->

</table>
</div>
